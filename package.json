{
  "name": "@vroskus/library-swagger",
  "version": "1.0.15",
  "description": "Swagger",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-swagger.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "npm run test:e2e:run && npm run test:e2e:post",
    "test:e2e:run": "jest --ci --colors",
    "test:e2e:post": "jest-coverage-thresholds-bumper"
  },
  "dependencies": {
    "@types/express": "4.17.21",
    "lodash": "4.17.21",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-problems": "8.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-newlines": "1.3.4",
    "eslint-plugin-perfectionist": "^2.6.0",
    "eslint-plugin-react": "^7.34.0",
    "express": "^4.18.3",
    "jest": "29.7.0",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 53.33,
        "functions": 30,
        "lines": 64.7,
        "statements": 59.45
      }
    }
  }
}
