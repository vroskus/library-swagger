{
  "name": "@vroskus/library-swagger",
  "version": "1.0.4",
  "description": "Swagger",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-swagger.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "npm run test:e2e:run && npm run test:e2e:post",
    "test:e2e:run": "jest --ci --colors",
    "test:e2e:post": "jest-coverage-thresholds-bumper"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.192",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-sort": "2.7.1",
    "express": "4.18.2",
    "jest": "29.5.0",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "npm-check": "6.0.1",
    "ts-jest": "29.1.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 25,
        "functions": 22.22,
        "lines": 56.66,
        "statements": 56.25
      }
    }
  }
}
