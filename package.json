{
  "name": "@vroskus/library-swagger",
  "version": "1.0.22",
  "description": "Swagger",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-swagger.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "npm run test:e2e:run && npm run test:e2e:post",
    "test:e2e:run": "jest --ci --colors",
    "test:e2e:post": "jest-coverage-thresholds-bumper"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "lodash": "^4.17.21",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@vroskus/eslint-config": "^1.0.21",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-coverage-thresholds-bumper": "^1.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 53.33,
        "functions": 30,
        "lines": 64.7,
        "statements": 59.45
      }
    }
  }
}
